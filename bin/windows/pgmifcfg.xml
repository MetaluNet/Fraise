<Config>
    <PgmIf name="PICPgm USB Programmer" typ="LVP" connection="USB">
        <PinCfg name="MCLR" pin="0" invert="1" />
        <PinCfg name="PGM" pin="3" invert="1" />
        <PinCfg name="CLK" pin="2" invert="1" />
        <PinCfg name="DOUT" pin="1" invert="1" />
        <PinCfg name="DIN" pin="4" invert="0" />
